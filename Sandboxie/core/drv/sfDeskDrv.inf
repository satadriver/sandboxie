DriverVer = 03/04/2022,11.48.34.896
CatalogFile = sfDeskDrv.cat

[DestinationDirs]
DefaultDestDir          = 12
MiniFilter.DriverFiles  = 12            ;%windir%\system32\drivers

[MiniFilter.DriverFiles]
sfDeskDrv.sys

[SourceDisksFiles]
sfDeskDrv.sys = 1,,

[SourceDisksNames]
1 = %DiskId1%,,,

